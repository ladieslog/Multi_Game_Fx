<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="randomNumber.ControllerRandomNumber">
   <children>
      <Label layoutX="177.0" layoutY="31.0" text="랜덤 숫자 맞추기 게임 !">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="num" layoutX="276.0" layoutY="121.0" prefHeight="30.0" prefWidth="75.0" />
      <Label layoutX="155.0" layoutY="123.0" text="숫자 입력">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label fx:id="checkText" alignment="CENTER" layoutX="163.0" layoutY="173.0" prefHeight="24.0" prefWidth="277.0" textFill="RED">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="174.0" layoutY="224.0" mnemonicParsing="false" onAction="#ranking" prefHeight="30.0" prefWidth="80.0" text="랭킹 확인" />
      <Button layoutX="326.0" layoutY="224.0" mnemonicParsing="false" onAction="#close" prefHeight="30.0" prefWidth="50.0" text="닫기" />
      <Button layoutX="383.0" layoutY="121.0" mnemonicParsing="false" onAction="#check" prefHeight="30.0" prefWidth="50.0" text="확인" />
   </children>
</AnchorPane>
